<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.dybcatering.live4teach">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppThemeRegist"
        android:networkSecurityConfig="@xml/network_security_config"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".Estudiante.Inicio.InicioActivity"></activity>
        <activity
            android:name=".Estudiante.Carrito.CarritoDetalleActivity"
            android:parentActivityName=".Estudiante.Carrito.CarritoActivity" />
        <activity android:name=".Splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Estudiante.Carrito.CarritoActivity"
            android:parentActivityName=".Estudiante.PrincipalActivity" />
        <activity android:name=".Estudiante.MisCursos.MisCursosDetalleVerMas" />
        <activity android:name=".Estudiante.MisCalificaciones.MisCalificacionesDetalle" />
        <activity android:name=".Estudiante.MisActividades.MisActividadesDetalle" />
        <activity
            android:name=".Estudiante.Perfil.ActualizarDatos"
            android:parentActivityName=".Estudiante.PrincipalActivity" />

        <activity android:name=".Estudiante.MisCertificados.MisCertificadosDetalle" />
        <!--    android:parentActivityName=".Estudiante.PrincipalActivity" /-->
        <activity
            android:name=".Estudiante.CursosDisponibles.CuartoCurso"
            android:parentActivityName=".Estudiante.PrincipalActivity" />
        <activity
            android:name=".Estudiante.CursosDisponibles.TercerCurso"
            android:parentActivityName=".Estudiante.PrincipalActivity" />
        <activity
            android:name=".Estudiante.CursosDisponibles.SegundoCurso"
            android:parentActivityName=".Estudiante.PrincipalActivity" />
        <activity
            android:name=".Estudiante.CursosDisponibles.PrimerCurso"
            android:parentActivityName=".Estudiante.PrincipalActivity" />
        <activity
            android:name=".Login.PreLoginActivity"
            android:theme="@style/AppThemeRegist.NoActionBar" />
        <activity android:name=".Login.HomeActivity" />
        <activity
            android:name=".Login.RegisterActivity"
            android:parentActivityName=".Login.PreLoginActivity"
            android:theme="@style/AppThemeRegist" />
        <activity
            android:name=".Login.LoginActivity"
            android:theme="@style/AppThemeLogin" />
        <activity
            android:name=".Estudiante.PrincipalActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppThemeRegist.NoActionBar" />
        <activity android:name=".Estudiante.SubPrimerActivity" />

        <activity android:name=".Tutor.InicioActivityTutor" />
    </application>


</manifest>